function playclick()

script.Parent = Player 

sound:play()

tyd.Visible = false

wait(0.1)

tya.Visible = true

end



function pauseclick()

script.Parent = Player 

sound:pause()

tyd.Visible = true

wait(0.1)

tya.Visible = false

end



function stopclick()

script.Parent = Player 

sound:stop()

tyd.Visible = true

tya.Visible = false

tyv.Visible = false

wait(0.1)

tyv.Visible = true

end



function plusclick()

script.Parent = Player 

sound.Volume = sound.Volume + 0.05 

local num = tonumber(tostring(sound.Volume):sub(1,4))

tee.Text = num 

if sound.Volume <= 0.0001 then return end 

tyz.Visible = false 

tyx.Visible = true 

end



function minusclick()

script.Parent = Player 

sound.Volume = sound.Volume - 0.05 

local num = tonumber(tostring(sound.Volume):sub(1,4))

tee.Text = num 

if sound.Volume >= 0.0001 then return end 

tyz.Visible = true 

tyx.Visible = false 

end



