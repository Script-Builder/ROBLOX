AddCommand("Hint","h","Makes Hint.","<Message>",
	function(Msg)
		coroutine.resume(coroutine.create(function()
			local a = Instance.new("Hint",Workspace)
			a.Text = "[ "..CO.Player.Name.." ]: "..Msg
			wait(5)
			a:Destroy()
		end))
	end
)
